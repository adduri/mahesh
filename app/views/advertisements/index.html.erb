<h2>Channel Partners</h2>
With best compliments from:<br/><br/>
<center>
<table width="250" height="200">
<% i = 0 %>
<% for r in 1..10 %>
  <tr>
	<% for c in 1..3 %>
    <td><% unless @advertisements[i].nil? or @advertisements.blank? %>
    	    <%= @advertisements[i].title %>
    	    <% unless @advertisements[i].ad.nil? or @advertisements[i].ad.blank? %>
    	    	<%= image_tag @advertisements[i].ad.url(:medium) %>
    	    <% end %>
    	<% end %>
        <% i = i + 1 %>
    </td>
    <% end %>
  </tr>
<% end %>
</table>
</center>


